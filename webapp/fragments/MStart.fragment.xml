<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        id="idMStartDialogLag"
        search="handleSearch"
        title="Start Month"
        titleAlignment="Center"
        liveChange="handleSearch"
        multiSelect="false"
        rememberSelections="true"
        confirm="handleSelection"
        selectionChange="handleSelectionChange"
        cancel="handleClose"
        items="{/results}"
        growing="false"
    >
        <StandardListItem
            title="{PERIODDESC}"
            description="{PERIODDESC}"
            type="Active"
        />
    </SelectDialog>
</core:FragmentDefinition>
