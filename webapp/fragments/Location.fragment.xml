<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        id="idLocDialogLag"
        search="handleSearch"
		liveChange="handleSearch"
        multiSelect="false"
        rememberSelections="true"
        confirm="handleSelection"
        cancel="handleClose"
        items="{/results}"
        growing="false"
        selectionChange="handleSelectionChange"
    >   
        <StandardListItem
            title="{LOCATION_ID}"
            description="{LOCATION_ID}"
            type="Active"
        />
    </SelectDialog>
</core:FragmentDefinition>
