<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        id="idAsmDialogLag"
        search="handleSearch"
        liveChange="handleSearch"
        multiSelect="true"
        rememberSelections="true"
        confirm="handleSelection"
        selectionChange="handleSelectionChange"
        cancel="handleClose"
        items="{/results}"
        growing="false"
    >
        <StandardListItem
            title="{ASSEMBLY}"
            description="{ASSEMBLY}"
            type="Active"
        />
    </SelectDialog>
</core:FragmentDefinition>
